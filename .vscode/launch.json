    "version": "0.2.0",
    "configurations": [
        {
            "name": "调试vuecli",
            "request": "launch",
            "type": "chrome",
            "url": "http://localhost:8888",
            "webRoot": "${workspaceFolder}/vuecli/testvueproject/src",
            "sourceMapPathOverrides": {
                "webpack:///./*": "${webRoot}/*",
                "webpack:///src/*": "${webRoot}/*",
                "webpack:///*": "*",
                "webpack:///./~/*": "${webRoot}/node_modules/*",
            },
        },
        {
            "name": "调试html中vue",
            "request": "launch",
            "type": "chrome",
            "file": "${workspaceFolder}/learnsrc/examples/index.html",
            "webRoot": "${workspaceFolder}/learnsrc/examples"
        } 
    ]
}